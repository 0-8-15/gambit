(declare (extended-bindings) (not constant-fold) (not safe))

(define (test1 a b)
  (let ((x (##flatan a)))
    (println (and (##fl>= x (##fl* 0.999999999999999 b))
                  (##fl<= x (##fl* 1.000000000000001 b))))))

(define (test2 a b c)
  (let ((x (##flatan a b)))
    (println (and (##fl>= x (##fl* 0.999999999999999 c))
                  (##fl<= x (##fl* 1.000000000000001 c))))))

(test1 0.0 0.0)
(test1 0.5 0.46364760900080615)
(test1 1.0 0.7853981633974483)
(test1 4.0 1.3258176636680326)

(test1 5.0 -9.999)

(test2  0.0  0.0  0.0)
(test2  0.0 -0.0  3.141592653589793)
(test2 -0.0  0.0 -0.0)
(test2 -0.0 -0.0 -3.141592653589793)

(test2  0.5  1.0  0.4636476090008061)
(test2  0.5 -1.0  2.677945044588987)
(test2 -0.5  1.0 -0.4636476090008061)
(test2 -0.5 -1.0 -2.677945044588987)
